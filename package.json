{
  "name": "@vitrin/web-video-player",
  "version": "0.2.0",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./styles": "./dist/index.css",
    "./types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve preview",
    "watch": "microbundle --jsx React.createElement --jsxFragment React.Fragment --css-modules true watch",
