    "test": "exit 1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "microbundle": "^0.15.0",
    "react": "^18.1.0",
    "vite": "^2.9.7"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "react-use": "^17.3.2"
  }
}
